	for (var thread_id in disq_comments) {
		if (!disq_comments.hasOwnProperty(thread_id)) continue;

		var comments = disq_comments[thread_id];
		for (var id in comments) {
			if (!comments.hasOwnProperty(id)) continue;

			var comment_obj = comments[id];
			var $comment_holder = $('div[id=tr'+thread_id+']');
			var $comment_img = $('<td>').css('padding', '5px').css('width', '50px').css('vertical-align', 'top').attr('rowspan', '2').append($('<img>').attr('src', comment_obj.avatar).css('width', '48px').css('border-radius', '5px').attr('title', comment_obj.name + ' сказал'));
			var $comment_header = $('<td>').css('padding-left', '5px').css('vertical-align', 'top').append($('<span>').addClass('comment-author').css('line-height', '10px').css('color', 'rgb(0, 136, 204)').css('font-size', '13px').css('font-weight', '700').append(comment_obj.name));
			var $comment_content = $('<td>').css('padding', '5px').css('vertical-align', 'top').append($('<span>').addClass('comment-content').css('line-height', '16px').append(comment_obj.comment));
			var $comment_item = $('<table>').append($('<tr>').append($comment_img).append($comment_header)).append($('<tr>').append($comment_content));
			$comment_holder.append($('<div>').addClass('col-xs-9 col-sm-9 col-md-9 col-lg-9').append($comment_item));
		}
	}
<!--/script-->

		});
